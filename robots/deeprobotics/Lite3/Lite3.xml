<mujoco model="Lite3">
  <compiler angle="radian"/>

  <default>
    <default class="Lite3">
      <joint damping="0.05" frictionloss="0.1"/>
    </default>
    <default class="visual">
      <geom type="mesh" contype="0" conaffinity="0"/>
    </default>
    <default class="collision">
      <geom group="3" friction="0.6" margin="0.001" condim="3"  contype="2" conaffinity="1"/>
      <!-- <default class="foot">
        <geom type="sphere" size="0.022" rgba="1 1 1 1" friction="1.0 0.3 0.3" />
      </default> -->
      <default class="foot">
        <!-- 尺寸改为0.03m提升稳定性 -->
        <!-- 优化：提升关键摩擦参数 -->
        <!-- 浅灰色橡胶材质视觉提示 -->
        <!-- 接触响应优化（尺寸增大后微调） -->
        <!-- 接触特性：匹配更大球体 -->
        <!-- 防穿透容差增加 -->
        <geom type="sphere" size="0.033"       
              friction="1.0 0.45 0.35"                      
              solref="0.02 1.2"   
              solimp="0.9 0.5 0.01"              
              condim="4"      
              priority="1"/>                   
      </default>
    </default>
    <default class="foot_site">
      <site size="0.05" group="3"/>
    </default>
  </default>

  <asset>
    <texture type="2d" name="Metal009_color"      file="../../../textures/Metal009_2K-PNG/Metal009_2K-PNG_Color.png"/>
    <texture type="2d" name="Metal009_roughness"  file="../../../textures/Metal009_2K-PNG/Metal009_2K-PNG_Roughness.png"/>
    <texture type="2d" name="Metal009_metalic"  file="../../../textures/Metal009_2K-PNG/Metal009_2K-PNG_Metalness.png"/>
    <texture type="2d" name="Metal009_normal"     file="../../../textures/Metal009_2K-PNG/Metal009_2K-PNG_NormalDX.png"/>

    <material name="pbr_metal_gray">
      <layer role="rgb"        texture="Metal009_color"/>
      <layer role="normal"     texture="Metal009_normal"/>
      <layer role="roughness"  texture="Metal009_roughness"/>
      <layer role="metallic"  texture="Metal009_metalic"/>
    </material>

    <texture type="2d" name="Metal044A_color"      file="../../../textures/Metal044A_2K-PNG/Metal044A_2K-PNG_Color.png"/>
    <texture type="2d" name="Metal044A_roughness"  file="../../../textures/Metal044A_2K-PNG/Metal044A_2K-PNG_Roughness.png"/>
    <texture type="2d" name="Metal044A_metalic"  file="../../../textures/Metal044A_2K-PNG/Metal044A_2K-PNG_Metalness.png"/>
    <texture type="2d" name="Metal044A_normal"     file="../../../textures/Metal044A_2K-PNG/Metal044A_2K-PNG_NormalDX.png"/>

    <material name="pbr_metal_silver_smooth">
      <layer role="rgb"        texture="Metal044A_color"/>
      <layer role="normal"     texture="Metal044A_normal"/>
      <layer role="roughness"  texture="Metal044A_roughness"/>
      <layer role="metallic"  texture="Metal044A_metalic"/>
    </material>

    <texture type="2d" name="Metal044B_color"      file="../../../textures/Metal044B_2K-PNG/Metal044B_2K-PNG_Color.png"/>
    <texture type="2d" name="Metal044B_roughness"  file="../../../textures/Metal044B_2K-PNG/Metal044B_2K-PNG_Roughness.png"/>
    <texture type="2d" name="Metal044B_metalic"  file="../../../textures/Metal044B_2K-PNG/Metal044B_2K-PNG_Metalness.png"/>
    <texture type="2d" name="Metal044B_normal"     file="../../../textures/Metal044B_2K-PNG/Metal044B_2K-PNG_NormalDX.png"/>

    <material name="pbr_metal_silver_rough">
      <layer role="rgb"        texture="Metal044B_color"/>
      <layer role="normal"     texture="Metal044B_normal"/>
      <layer role="roughness"  texture="Metal044B_roughness"/>
      <layer role="metallic"  texture="Metal044B_metalic"/>
    </material>    

    <texture type="2d" name="Plastic006_color"      file="../../../textures/Plastic006_2K-PNG/Plastic006_2K-PNG_Color.png"/>
    <texture type="2d" name="Plastic006_roughness"  file="../../../textures/Plastic006_2K-PNG/Plastic006_2K-PNG_Roughness.png"/>
    <texture type="2d" name="Plastic006_normal"     file="../../../textures/Plastic006_2K-PNG/Plastic006_2K-PNG_NormalDX.png"/>

    <material name="pbr_plastic_black" metallic ="0.05">
      <layer role="rgb"        texture="Plastic006_color"/>
      <layer role="normal"     texture="Plastic006_normal"/>
      <layer role="roughness"  texture="Plastic006_roughness"/>
    </material>

    <texture type="2d" name="Plastic013A_color"      file="../../../textures/Plastic013A_2K-PNG/Plastic013A_2K-PNG_Color.png"/>
    <texture type="2d" name="Plastic013A_roughness"  file="../../../textures/Plastic013A_2K-PNG/Plastic013A_2K-PNG_Roughness.png"/>
    <texture type="2d" name="Plastic013A_normal"     file="../../../textures/Plastic013A_2K-PNG/Plastic013A_2K-PNG_NormalDX.png"/>

    <material name="pbr_plastic_white" metallic ="0.05">
      <layer role="rgb"        texture="Plastic013A_color"/>
      <layer role="normal"     texture="Plastic013A_normal"/>
      <layer role="roughness"  texture="Plastic013A_roughness"/>
    </material>

    <texture type="2d" name="Plastic010_color"      file="../../../textures/Plastic010_2K-PNG/Plastic010_2K-PNG_Color.png"/>
    <texture type="2d" name="Plastic010_roughness"  file="../../../textures/Plastic010_2K-PNG/Plastic010_2K-PNG_Roughness.png"/>
    <texture type="2d" name="Plastic010_normal"     file="../../../textures/Plastic010_2K-PNG/Plastic010_2K-PNG_NormalDX.png"/>

    <material name="pbr_plastic_gray" metallic ="0.05">
      <layer role="rgb"        texture="Plastic010_color"/>
      <layer role="normal"     texture="Plastic010_normal"/>
      <layer role="roughness"  texture="Plastic010_roughness"/>      
    </material>

    <mesh name="torso" file="torso.STL"/>
    <mesh name="fr_thigh" file="fr_thigh.STL"/>
    <mesh name="fr_shank" file="fr_shank.STL"/>
    <mesh name="fr_hip" file="fr_hip.STL"/>

    <mesh name="fl_thigh" file="fl_thigh.STL"/>
    <mesh name="fl_shank" file="fl_shank.STL"/>
    <mesh name="fl_hip" file="fl_hip.STL"/>
    
    <mesh name="hl_thigh" file="hl_thigh.STL"/>
    <mesh name="hl_shank" file="hl_shank.STL"/>
    <mesh name="hl_hip" file="hl_hip.STL"/>
    
    <mesh name="hr_thigh" file="hr_thigh.STL"/>
    <mesh name="hr_shank" file="hr_shank.STL"/>
    <mesh name="hr_hip" file="hr_hip.STL"/>
  </asset>

  <worldbody>
    <body name="command_indicator_mocap" pos="0 0 0" mocap="true">
      <geom conaffinity="0" contype="0" pos="0.3 0 0" rgba="0.7 0 0 0.5" size="0.4 0.005 0.005" type="box" group="1"></geom>
      <geom conaffinity="0" contype="0" pos="0 0.3 0" rgba="0 0.7 0 0.5" size="0.005 0.4 0.005" type="box" group="1"></geom>
      <geom conaffinity="0" contype="0" pos="0 0 0.3" rgba="0 0 0.7 0.5" size="0.005 0.005 0.4" type="box" group="1"></geom>
    </body>

    <body name="torso" pos="0 0 0.5" childclass="Lite3">
      <freejoint name="torso" />
      <geom type="mesh" mesh="torso" material="pbr_plastic_white" class="visual"/>
      <geom type="box" size="0.28 0.12 0.04" pos="0 0 0.025" class="collision"/>
      <inertial pos="0 0 0" mass="5.6056" diaginertia="0.02456 0.05518 0.07016"/>
      <site name="imu" pos="0 0 0" size="0.01"/>

      <body name="FL_HIP" pos="0.1745 0.062 0">
        <geom type="mesh" mesh="fl_hip" pos="0 0 0" class="visual" material="pbr_metal_gray"/>
        <inertial pos="-0.00601 -0.0066532 0.00034295" quat="0 0.707107 0 0.707107" mass="0.55" diaginertia="0.0004472 0.0004028 0.0003949"/>
        <joint name="FL_HipX_joint" pos="0 0 0" axis="-1 0 0" range="-0.523 0.523" actuatorfrcrange="-24 24"/>
        <body name="FL_THIGH" pos="0 0.09735 0">
          <geom type="mesh" mesh="fl_thigh" pos="0 0 0" class="visual" rgba="0.5 0.5 0.5 1" material="pbr_plastic_white"/>
          <inertial pos="-0.0052817 -0.014632 -0.042672" mass="0.86" diaginertia="0.005736 0.00496 0.001436"/>
          <joint name="FL_HipY_joint" pos="0 0 0" axis="0 -1 0" range="-2.67 0.314" actuatorfrcrange="-24 24"/>
          <geom size="0.03 0.02 0.12" pos="0 0 -0.08" euler="0 0.1 0" type="box" rgba="0.5 0.5 0.5 1" class="collision"/>
          <body name="FL_SHANK" pos="0 0 -0.2">
            <inertial pos="0.00573034 -1.28547e-06 -0.131807" quat="0.707101 0.00359881 0.00359725 0.707095" mass="0.173" diaginertia="0.00104615 0.00102919 3.19053e-05"/>
            <joint name="FL_Knee_joint" pos="0 0 0" axis="0 -1 0" range="0.524 2.792" actuatorfrcrange="-36 36"/>
            <geom size="0.01 0.01 0.09" pos="0.01 0 -0.09" type="box" rgba="0.2 0.2 0.2 1"  class="collision"/>
            <geom type="mesh" mesh="fl_shank" pos="0 0 0" class="visual" rgba="0.2 0.2 0.2 1" material="pbr_metal_gray" />
            <body name="FL_FOOT" pos="0 0 -0.21012">
              <inertial pos="0 0 0" mass="0.02" diaginertia="0 0 0"/>
              <geom name="FL_FOOT" class="foot"/>
              <site name="FL_site" class="foot_site" />
            </body>
          </body>
        </body>
      </body>

      <body name="FR_HIP" pos="0.1745 -0.062 0">
      
        <geom type="mesh" mesh="fr_hip" pos="0 0 0" class="visual" material="pbr_metal_gray"/>
        <inertial pos="-0.010579 0.011358 0.00048546" mass="0.55" diaginertia="0.0004472 0.0004028 0.0003949"/>
        <joint name="FR_HipX_joint" pos="0 0 0" axis="-1 0 0" range="-0.523 0.523" actuatorfrcrange="-24 24"/>
        <body name="FR_THIGH" pos="0 -0.09735 0">
          <geom type="mesh" mesh="fr_thigh" pos="0 0 0" class="visual" rgba="0.5 0.5 0.5 1" material="pbr_plastic_white"/>
          <inertial pos="-0.0039245 0.025256 -0.025146" mass="0.86" diaginertia="0.005736 0.00496 0.001436"/>
          <joint name="FR_HipY_joint" pos="0 0 0" axis="0 -1 0" range="-2.67 0.314" actuatorfrcrange="-24 24"/>
          <geom size="0.03 0.02 0.12" pos="0 0 -0.08" euler="0 0.1 0" type="box" rgba="0.5 0.5 0.5 1" class="collision"/>
          <body name="FR_SHANK" pos="0 0 -0.2">
            <geom type="mesh" mesh="fr_shank" pos="0 0 0" class="visual" rgba="0.2 0.2 0.2 1"  material="pbr_metal_gray" />
            <inertial pos="0.00573034 -1.28697e-06 -0.131807" quat="0.707101 0.00359881 0.00359725 0.707095" mass="0.173" diaginertia="0.00104615 0.00102919 3.19053e-05"/>
            <joint name="FR_Knee_joint" pos="0 0 0" axis="0 -1 0" range="0.524 2.792" actuatorfrcrange="-36 36"/>       
            <geom size="0.01 0.01 0.09" pos="0.01 0 -0.09" type="box" rgba="0.2 0.2 0.2 1" class="collision"/>
            <body name="FR_FOOT" pos="0 0 -0.21012">
              <inertial pos="0 0 0" mass="0.02" diaginertia="0 0 0"/>
              <geom name="FR_FOOT" class="foot"/>
              <site name="FR_site" class="foot_site" />
            </body>
          </body>
        </body>
      </body>
      
      <body name="HL_HIP" pos="-0.1745 0.062 0">
        <geom type="mesh" mesh="hl_hip" pos="0 0 0" class="visual" material="pbr_metal_gray"/>
        <inertial pos="0.010905 -0.012636 0.001051" quat="0 0.707107 0 0.707107" mass="0.55" diaginertia="0.0004472 0.0004028 0.0003949"/>
        <joint name="HL_HipX_joint" pos="0 0 0" axis="-1 0 0" range="-0.523 0.523" actuatorfrcrange="-24 24"/>
        <body name="HL_THIGH" pos="0 0.09735 0">
          <geom type="mesh" mesh="hl_thigh" pos="0 0 0" class="visual" rgba="0.5 0.5 0.5 1" material="pbr_plastic_white"/>
          <inertial pos="-0.0057137 -0.014631 -0.042453" mass="0.86" diaginertia="0.005736 0.00496 0.001436"/>
          <joint name="HL_HipY_joint" pos="0 0 0" axis="0 -1 0" range="-2.67 0.314" actuatorfrcrange="-24 24"/>
          <geom size="0.03 0.02 0.12" pos="0 0 -0.08" euler="0 0.1 0" type="box" rgba="0.5 0.5 0.5 1" class="collision"/>
          <body name="HL_SHANK" pos="0 0 -0.2">
            <geom type="mesh" mesh="hl_shank" pos="0 0 0" class="visual" rgba="0.2 0.2 0.2 1" material="pbr_metal_gray" />
            <inertial pos="0.00573034 -1.2875e-06 -0.131807" quat="0.707101 0.00359881 0.00359725 0.707095" mass="0.173" diaginertia="0.00104615 0.00102919 3.19053e-05"/>
            <joint name="HL_Knee_joint" pos="0 0 0" axis="0 -1 0" range="0.524 2.792" actuatorfrcrange="-36 36"/>
            <geom size="0.01 0.01 0.09" pos="0.01 0 -0.09" type="box" rgba="0.2 0.2 0.2 1" class="collision"/>
            <body name="HL_FOOT" pos="0 0 -0.21012">
              <inertial pos="0 0 0" mass="0.02" diaginertia="0 0 0"/>
              <geom name="HL_FOOT" class="foot"/>
              <site name="HL_site" class="foot_site" />
            </body>
          </body>
        </body>
      </body>
      
      <body name="HR_HIP" pos="-0.1745 -0.062 0">
        <geom type="mesh" mesh="hr_hip" pos="0 0 0" class="visual" material="pbr_metal_gray"/>
        <inertial pos="0.010354 0.011423 0.00049498" quat="0 0.707107 0 0.707107" mass="0.55" diaginertia="0.0004472 0.0004028 0.0003949"/>
        <joint name="HR_HipX_joint" pos="0 0 0" axis="-1 0 0" range="-0.523 0.523" actuatorfrcrange="-24 24"/>
        <body name="HR_THIGH" pos="0 -0.09735 0">
          <geom type="mesh" mesh="hr_thigh" pos="0 0 0" class="visual" rgba="0.5 0.5 0.5 1" material="pbr_plastic_white"/>
          <inertial pos="-0.0031903 0.02526 -0.025617" mass="0.86" diaginertia="0.005736 0.00496 0.001436"/>
          <joint name="HR_HipY_joint" pos="0 0 0" axis="0 -1 0" range="-2.67 0.314" actuatorfrcrange="-24 24"/>
          <geom size="0.03 0.02 0.12" pos="0 0 -0.08" euler="0 0.1 0" type="box" rgba="0.5 0.5 0.5 1" class="collision"/>
          <body name="HR_SHANK" pos="0 0 -0.2">
            <geom type="mesh" mesh="hr_shank" pos="0 0 0" class="visual" rgba="0.2 0.2 0.2 1" material="pbr_metal_gray" />
            <inertial pos="0.00573034 -1.28493e-06 -0.131807" quat="0.707101 0.00359881 0.00359725 0.707095" mass="0.173" diaginertia="0.00104615 0.00102919 3.19053e-05"/>
            <joint name="HR_Knee_joint" pos="0 0 0" axis="0 -1 0" range="0.524 2.792" actuatorfrcrange="-36 36"/>
            <geom size="0.01 0.01 0.09" pos="0.01 0 -0.09" type="box" rgba="0.2 0.2 0.2 1" class="collision"/>
            <body name="HR_FOOT" pos="0 0 -0.21012">
              <inertial pos="0 0 0" mass="0.02" diaginertia="0 0 0"/>
              <geom name="HR_FOOT" class="foot"/>
              <site name="HR_site" class="foot_site" />
            </body>
          </body>
        </body>
      </body>
    </body>
  </worldbody>

  <actuator>
    <!-- Front Left Leg -->
    <motor joint="FL_HipX_joint" name="FL_HipX_actuator" ctrlrange="-24 24"/>
    <motor joint="FL_HipY_joint" name="FL_HipY_actuator" ctrlrange="-24 24"/>
    <motor joint="FL_Knee_joint" name="FL_Knee_actuator" ctrlrange="-36 36"/>
    
    <!-- Front Right Leg -->
    <motor joint="FR_HipX_joint" name="FR_HipX_actuator" ctrlrange="-24 24"/>
    <motor joint="FR_HipY_joint" name="FR_HipY_actuator" ctrlrange="-24 24"/>
    <motor joint="FR_Knee_joint" name="FR_Knee_actuator" ctrlrange="-36 36"/>
    
    <!-- Hind Left Leg -->
    <motor joint="HL_HipX_joint" name="HL_HipX_actuator" ctrlrange="-24 24"/>
    <motor joint="HL_HipY_joint" name="HL_HipY_actuator" ctrlrange="-24 24"/>
    <motor joint="HL_Knee_joint" name="HL_Knee_actuator" ctrlrange="-36 36"/>
    
    <!-- Hind Right Leg -->
    <motor joint="HR_HipX_joint" name="HR_HipX_actuator" ctrlrange="-24 24"/>
    <motor joint="HR_HipY_joint" name="HR_HipY_actuator" ctrlrange="-24 24"/>
    <motor joint="HR_Knee_joint" name="HR_Knee_actuator" ctrlrange="-36 36"/>
  </actuator>

  <sensor>
    <framequat name="imu_quat" objtype="site" objname="imu"/>

    <touch name="FL_touch" site="FL_site"/>
    <touch name="FR_touch" site="FR_site"/>
    <touch name="HL_touch" site="HL_site"/>
    <touch name="HR_touch" site="HR_site"/>
  </sensor>
</mujoco>
